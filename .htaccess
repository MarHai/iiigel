RewriteEngine On

RewriteBase /iiigel

RewriteRule ^config/.*$ - [L]
RewriteRule ^src/.*$ - [L]
RewriteRule ^vendor/.*$ - [L]

RewriteRule ^upload/.*$ - [F]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-l

RewriteRule ^([^?]*)$ index.php?path=$1 [NC,L,QSA]
